function bagjen() --every good tetris game needs 7bag support
	for k,v in pairs(basebag) do
		buf_bag[k] = v
	end
	bag = {}
	filled = false
	i = 1
	while not filled do
		buf_val = rnd(buf_bag)
		bag[i] = buf_val
		del(buf_bag,buf_val)
		i += 1
		if #buf_bag==0 then
			filled = true
		end
	end
	piece = bag[#bag]
end
