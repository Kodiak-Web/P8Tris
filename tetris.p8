mergetime = 45
function move(x,y) 
	colliding=false
	coltest(x,y,0)
	if colliding == false then
		posx += x
		posy += y

	elseif y==1 and coltimer < mergetime then
		coltimer += increments / 3
	elseif y==1 and coltimer >=mergetime then
		coltimer = 0
		merge()
		merged = true
	end
end
