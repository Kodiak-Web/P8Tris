	for k,v in pairs(piece_pointer[rot_val]) do
		for kk,vv in pairs(v) do
			smap(vv,(kk*6)+(posx*6),(k*6)+(posy*6))
		end
	end
	print(stat(2),2)
	print(LinesCleared,0,10,2)
end

