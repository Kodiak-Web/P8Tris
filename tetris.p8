function drop() --moves piece down and spams coltest until it hits something.
		i = 1
		finished = false
		while not finished do
			if coltest(0,i,0) or i > 22 then
				move(0,i-1)
				merge()
				finished = true
			end
			i += 1
		end
		merged = true
	merged = false
	coltimer = mergetime
	--clearline(1)	
end



function rotate(n)
	--	if piece == 0 then
	--	 --I logic
	--	else
		not_rotated = true
		tablerot = nil
		if n == -1 then tablerot = 1
		elseif n==1 then tablerot=2
		end
		coltimer -= flr(increments/1.5) --honestly forgot what this was for, i think its used for gravity? TODO: figure out why i did this and fix my shitty gravity implementation
		if coltest(0,0,n)==false then 
			rot_val += n
			if rot_val > 4 then
				rot_val = 1
			elseif rot_val < 1 then
				rot_val = 4
			end
		else
			kick_test = 1
			kicking = true
			tablepoint = kick_table[rot_val][tablerot]
			while kicking do
				collides = coltest(tablepoint[kick_test][1],(tablepoint[kick_test][2] * -1),n)
				if collides == false then
					rot_val += n
					if rot_val > 4 then
						rot_val = 1
						elseif rot_val < 1 then
						rot_val = 4
					end
					posx += tablepoint[kick_test][1]
					posy += (tablepoint[kick_test][2] * -1) 
					kicking = false
				elseif kick_test < 4 then
					kick_test +=1
				else
					kicking = false
				end
			end
		end
end
