function coltest(tx,ty, trot)
	colliding=false
	r = rot_val + trot
	if r == 0 then r = 4 end
	if r == 5 then r = 1 end
	for k,v in pairs(piece_pointer[r]) do
		for kk,vv in pairs(v) do
			last_k = k --debugging var
			y = k + posy + ty
			x = kk+ posx + tx
			if collision_field[y][x] !=0 and piece_pointer[r][k][kk] !=0 then
				colliding=true
			end
		end
	end
	return colliding
end
