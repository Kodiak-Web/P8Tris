function merge()
	for k,v in pairs(piece_pointer[rot_val]) do
		for kk,vv in pairs(v) do
			y = k + posy
			x = kk+ posx
			if vv != 0 then
				collision_field[y][x] = piece_pointer[rot_val][k][kk]
			end
		end
	end
	line_detect()
	posx=5
	posy=0
	piece = ceil(rnd(7))
	piece_pointer = piece_table[piece]
end
function line_detect()
	for k,v in pairs(collision_field) do
		filled = true
		for kk = 1, #v-4 do
			if 0 == v[kk] or 8==v[kk] then --detects filled lines. 0 is "empty", 8 is wall.
				filled=false
			end
		end
		if filled then 
			clearline(k) 
			LinesCleared += 1
		end
	end
end
line = 0
function clearline(n)
	line = n
	loop1 = true
	while loop1==true do
		collision_field[line] = collision_field[line - 1]
		line -= 1
		if line == 0 then
			collision_field[0] = {}
			for x= -3,0 do
				collision_field[0][x] = 8
			end
			for x=1,10 do   
				collision_field[0][x] = 0
			end
			for x=11,14 do                   
				collision_field[0][x] = 8       
			end
			loop1 = false
		end
	end
end
