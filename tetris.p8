bms = 3
brms = 10
brmscounter = {0,0,0,0,0}
bmscounter = {0,0,0,0,0}
function btna(i)
	i = i + 1
	pressed = false
	if btn(i-1) then
		if brmscounter[i] == 0 then pressed=true end
		if brmscounter[i] < brms then
			brmscounter[i] += 1
		elseif bmscounter[i] < bms then
			bmscounter[i] += 1
		else pressed=true end
	else brmscounter[i] = 0 end
	return pressed
end
dflag = false
function btnd()
	if btn(2) and dflag==false then
		dtag=true
		pressed = true
	elseif not btn(2) and dflag==true then
		dtag = false
		pressed = false
	else if btn(2) and dflag==true then
		dtag=false
		pressed = false
	end end
	return pressed
end
function _update()
	if (btna(0))  move(-1,0)
	if (btna(1))  move(1,0)
	if (btna(2))  drop()
	if (btna(3))  move(0,1)
	if (btna(4)) rotate(-1)
	if (btna(5)) rotate(1)
	if timer < increments - (LinesCleared / 2) then timer += 1 
	else 
		move(0,1)
		timer = 0
	end 
	--coltest(0,0)
end

function _draw()
